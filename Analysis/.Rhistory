euc_dist <- function(x){
# convert to matrix 2 x n matrix
x = matrix(x, nrow = 2)
d = 0
for(ii in 1:ncol(x)){
d = d + abs((x[1, ii] - x[2, ii]))^2
}
d = sqrt(d)
return(d)
}
